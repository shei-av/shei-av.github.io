{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "data": {
       "url": "https://raw.githubusercontent.com/shei-av/shei-av.github.io/main/indle.csv"},


    "title": {
        "text": "Life Expectancy Indonesia",
        "subtitle": "United Kingdom (thousands) seasonally adjusted",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},

    "width":300,
    "height":300,

    "mark":{
        "type": "bar",
        "color":"purple",
        "strokeWidth":2,
        "opacity":1},
 
    "encoding": {
      "x": {
          "field": "Year",
          "type": "temporal",
          "title":null},
      "y": {
          "field": "Value",
          "type": "quantitative",
          "title":null}
        
    }
}
